    {
      "name": "Sven Reifschneider",
      "homepage": "https://neoground.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "monolog/monolog": "^3.5",
    "nesbot/carbon": "^2.72",
    "symfony/console": "^6.4",
    "twig/twig": "^3.8",
    "phroute/phroute": "^2.2",
    "predis/predis": "^2.2",
    "symfony/yaml": "^6.4",
    "dragonmantank/cron-expression": "^3.3",
    "phpmailer/phpmailer": "^6.9",
    "league/flysystem": "^3.24",
    "league/flysystem-sftp-v3": "^3.23",
    "opis/closure": "^3.6",
    "claviska/simpleimage": "^4.0",
    "illuminate/database": "^10.46",
    "guzzlehttp/guzzle": "^7.8",
    "respect/validation": "^2.3"
  },
  "require-dev": {
    "filp/whoops": "^2.15",
    "kint-php/kint": "^5.1",
    "maximebf/debugbar": "^1.21",
    "phpunit/phpunit": "^10.5"
  },
  "suggest": {
    "ext-redis": "For native redis support instead of using predis",
